{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "assets": {
      "type": "library",
      "root": "libs/assets",
      "entryFile": "index",
      "sourceRoot": "libs/assets/src",
      "compilerOptions": {
        "tsConfigPath": "libs/assets/tsconfig.lib.json",
        "webpack": true,
        "webpackConfigPath": "./webpack.config.js"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "authors-service": {
      "type": "application",
      "root": "apps/authors-service",
      "entryFile": "main",
      "sourceRoot": "apps/authors-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authors-service/tsconfig.app.json"
      }
    },
    "books-service": {
      "type": "application",
      "root": "apps/books-service",
      "entryFile": "main",
      "sourceRoot": "apps/books-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/books-service/tsconfig.app.json"
      }
    },
    "borrow-service": {
      "type": "application",
      "root": "apps/borrow-service",
      "entryFile": "main",
      "sourceRoot": "apps/borrow-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/borrow-service/tsconfig.app.json"
      }
    },
    "genres-service": {
      "type": "application",
      "root": "apps/genres-service",
      "entryFile": "main",
      "sourceRoot": "apps/genres-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/genres-service/tsconfig.app.json"
      }
    },
    "publishers-service": {
      "type": "application",
      "root": "apps/publishers-service",
      "entryFile": "main",
      "sourceRoot": "apps/publishers-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/publishers-service/tsconfig.app.json"
      }
    },
    "users-service": {
      "type": "application",
      "root": "apps/users-service",
      "entryFile": "main",
      "sourceRoot": "apps/users-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users-service/tsconfig.app.json"
      }
    }
  }
}

