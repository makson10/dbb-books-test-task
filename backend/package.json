    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "config": "^4.1.1",
    "dotenv": "^17.2.1",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
